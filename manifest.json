{
  "manifest_version": 3,
  "name": "Terminimal",
  "description": "A terminal-like new tab extension for Chrome",
  "version": "1.0",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
